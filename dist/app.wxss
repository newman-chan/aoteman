view,
text {
  font-family: 'PingFang SC Regular', 'PingFang-SC-Regular', '微软雅黑', 'Microsoft YaHei', -apple-system-font, 'Helvetica Neue', 'Hiragino Sans GB', sans-serif;
}
page,
view,
text,
scroll-view,
swiper,
swiper-item,
picker {
  box-sizing: border-box;
}
.page,
page {
  position: relative;
  height: 100%;
}
scroll-view,
.scroll-cont {
  position: relative;
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.bg {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.icon-arrow {
  position: absolute;
  top: 0;
  right: 0;
  display: block;
  width: 100rpx;
  height: 100rpx;
  padding-right: 40rpx;
  text-align: right;
}
.icon-arrow .iconfont {
  display: block;
  font-size: 24rpx;
  color: #ffffff;
}
.icon-arrow ~ input {
  padding-right: 60rpx;
}
.hide {
  display: none !important;
}
.ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.ellipsis2 {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.ani {
  transition: all 0.3s ease-in-out;
}
.cardFilter {
  -webkit-animation: cardFilter 3s linear 0ms 1;
}
@-webkit-keyframes cardFilter {
  50% {
    -webkit-filter: brightness(2.5) sepia(0.5);
  }
  100% {
    -webkit-filter: none;
  }
}
.page {
  position: fixed;
  width: 100%;
  height: 100%;
  overflow: hidden;
}
